<div id="<%= dom_id client %>">
  <tr>
    <th scope="row"><%= client.id %></th>
    <td><%= client.name %></td>
    <td><%= client.age %></td>
    <td>
      <% if client.movies.present? %>
        <%= client.movies.last.name %>
      <% else %>
        No Movies
      <% end %>
    </td>
    <td><%= link_to "Show this client", client %></td>
  </tr>
</div>